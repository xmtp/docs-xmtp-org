{
  "name": "Docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vocs build",
    "dev": "vocs dev",
    "lint:md:fix": "npm run prettier:fix && markdownlint --fix 'docs/**/*.md' 'docs/**/*.mdx'",
    "lint:md": "npm run prettier:lint && markdownlint 'docs/**/*.md' 'docs/**/*.mdx'",
    "prettier:fix": "npm run prettier:lint -- --write",
    "prettier:lint": "prettier --ignore-path .gitignore docs/pages",
    "preview": "vocs preview",
    "spell": "cspell"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "mermaid": "^11.9.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-medium-image-zoom": "^5.2.14",
    "react-router": "^7.8.2",
    "vocs": "^1.0.13"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "cspell": "^9.1.3",
    "markdownlint-cli": "^0.45.0",
    "markdownlint-rule-search-replace": "^1.2.0",
    "typescript": "latest"
  }
}